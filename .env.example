# Gemini API Configuration
# メインAPIキー（必須）
GEMINI_API_KEY=your_primary_gemini_api_key_here

# 追加のAPIキー（ローテーション用・オプション）
# 複数のAPIキーを設定することで、レート制限を回避できます
GEMINI_API_KEY_1=your_secondary_gemini_api_key_here
GEMINI_API_KEY_2=your_tertiary_gemini_api_key_here
GEMINI_API_KEY_3=your_quaternary_gemini_api_key_here

# Application Settings
APP_HOST=127.0.0.1
APP_PORT=8001
DEBUG=True

# Database
DATABASE_URL=sqlite:///./data/lectures.db

# Search Configuration
MAX_SEARCH_RESULTS=10
SEARCH_TIMEOUT=30
